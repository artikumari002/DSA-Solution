import java.util.*;  
import java.io.*;  


public class Solution{
    public static int largestElement(int[] arr, int n){
        int largest = arr[0];
        for(int i = 0; i < n; i++){
            if(arr[i] > largest){
                largest = arr[i];
            }
        }
        return largest;
    }


    public static void main(String[] args){
    int[] arr = {1,2,3,4,5};
    int n = arr.length;
    System.out.println ("Largest Element : " + largestElement(arr,n));
    }

}







